# quickdbd file

users
-
userID int PK
firstName VARCHAR
middleName VARCHAR
lastName VARCHAR
role int FK - role.roleid
login VARCHAR
password VARCHAR

role
-
roleID int PK
role VARCHAR

tests
-
testID int PK
name VARCHAR
owner int FK - users.userid

questions
-
questionID int PK
name VARCHAR
answers VARCHAR
test int FK - tests.testid

solved_tests
-
solved_test_id int PK
user int FK - users.userid
test int FK - tests.testid
true_answers int
false_answers int
mark null int

answers
-
answerID int PK
user int FK - users.userid
test int FK - tests.testid
question int FK - questions.questionid
answer VARCHAR
mark bool